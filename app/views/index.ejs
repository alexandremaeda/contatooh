<!DOCTYPE html>
<html ng-app="contatooh" ng-controller="HomeController as ctrlHome">
<head>
	<meta charset="utf-8">
	<title>Contatooh</title>

	<base href='/'>

	<meta property="og:url" content="{{ctrlHome.url}}">
	<meta property="og:title" content="{{ctrlHome.title}}">
	<meta property="og:description" content="{{ctrlHome.description}}">
	<meta property="og:image" content="{{ctrlHome.image}}">
	<meta property="og:image:type" content="{{ctrlHome.imageType}}">
	<meta property="og:image:width" content="{{ctrlHome.imageWidth}}"> 
	<meta property="og:image:height" content="{{ctrlHome.imageHeight}}"> 

	<meta name="fragment" content="!">
	<link rel="stylesheet" href="vendor/bootstrap/dist/css/bootstrap.css">
	<link rel="stylesheet" href="vendor/bootstrap/dist/css/bootstrap-theme.css">
</head>
<body class="container">
	<%if (usuarioAutenticado.length > 0) {%>
	<div class="text-right">
		<p>
			Autenticado como: 
			<span id="usuarioAutenticado">
				<%=usuarioAutenticado%>
			</span>
			<a href="/logout">Desconectar</a>
		</p>
	</div>
	<%} %>
	<div ng-view></div>
</body>
<script src="vendor/angularjs/angular.js"></script>
<script src="vendor/angular-route/angular-route.js"></script>
<script src="vendor/angular-resource/angular-resource.js"></script>
<script src="js/main.js"></script>
<script src="js/controllers/homeController.js"></script>
<script src="js/controllers/contatosController.js"></script>
<script src="js/controllers/contatoController.js"></script>
<script src="js/services/interceptorService.js"></script>
<script src="js/services/contatoService.js"></script>
</html>